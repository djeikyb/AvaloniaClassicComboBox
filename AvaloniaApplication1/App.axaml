<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AvaloniaApplication1.App"
             RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- <MergeResourceInclude Source="/Controls/CooooomboBox.axaml" /> -->
                <MergeResourceInclude Source="/Controls/Copied/ComboBox.axaml" />
                <MergeResourceInclude Source="/Controls/Copied/ComboBoxItem.axaml" />
        <MergeResourceInclude Source="/Controls/TemplatedControl2.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    <Application.Styles>
        <FluentTheme DensityStyle="Compact" />
        <StyleInclude Source="avares://Avalonia.Controls.TreeDataGrid/Themes/Fluent.axaml" />
        <StyleInclude Source="/Controls/TemplatedControl1.axaml" />
    </Application.Styles>
</Application>
